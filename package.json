{
  "private": true,
  "scripts": {
    "start": "nuxt start",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "type-check": "tsc --pretty --noEmit",
    "eslint": "eslint --fix --max-warnings 0",
    "prettier": "prettier --write",
    "stylelint": "stylelint --fix",
    "vue-tsc": "vue-tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "yarn eslint",
      "yarn prettier"
